	Q:= {'I','R', "RO", "ROB" ,"ROBO", "ROBOT", "ROBOT_", "ROBOT_R" 
	,"V", "VA", "VAR", "VARS","VARSL",
	"B","BE","BEG","BEGI","BEGIN",
