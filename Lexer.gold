elseif q="BE" and σ= 'G' then return "BEG"  
elseif q="BEG" and σ= 'I' then return "BEGI"  
elseif q="BEGI" and σ= 'N' then return "BEGIN"
elseif q="BEGIN" and σ= 'M' then return "M"
