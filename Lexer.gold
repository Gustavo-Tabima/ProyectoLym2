elseif q ="CANMOVE:" and σ∈('a'‥'z')∪('1'‥'9') then return "CANMOVE:N"
elseif q="CANMOVE:N" then return "BEGIN"
		
// Not a IF

elseif q ="IF" and σ= 'N' then return "N"
elseif q ="N" and σ= 'O' then return "NO"
elseif q ="NO" and σ= 'T' then return "NOT"
